{% block modal_confirm %}
    <div class="modal fade" id="confirm" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Confirmación eliminación de usuarios</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Cerrar">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    ¿Seguro que quiere realizar esta operación?
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
                    <button type="button" class="btn btn-primary" id="btConfirmDelete">Confirmar</button>
                </div>
            </div>
        </div>
    </div>
    <div class="modal fade" id="confirmSimple" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Confirmación eliminación de usuario</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Cerrar">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    ¿Seguro que quiere eliminar a este usuario?
                </div>
                <div class="modal-footer">
                    <a class='invisible' id="enlace"></a>
                    <button type="button" class="btn btn-secondary" data-dismiss="modal" id="btCerrarmSimpleDelete">Cerrar</button>
                    <button type="button" class="btn btn-primary" id="btConfirmSimpleDelete">Confirmar</button>
                </div>
            </div>
        </div>
    </div>
{% endblock %}